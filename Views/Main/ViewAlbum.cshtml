  
<html>
    <body>
        <a href="upload_shots?id=@ViewBag.albumId">Upload Shots</a>
        <br>
        @foreach(var shot in @ViewBag.shots)  {
            <table style="display:inline">
                <tr><td><img width=200 height=200></td></tr>
                <tr><td>@shot.Name</td></tr>
                <tr><td>
                    <a href="/view_album?id=@shot.ShotId">View</a>&nbsp;
                    <a href="/edit_album?id=@shot.ShotId">Edit</a>&nbsp;
                    <a href="/delete_album?id=@shot.ShotId">Delete</a>
                </td></tr>                
            </table>
        }
    </body>    
</html>