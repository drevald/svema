@model IEnumerable<svema.Data.Album>
<html>
    <body>
        <a href="add_album">Add Album</a>
        <a href="upload_album">Upload Album</a>
        <br>
        @foreach(var album in Model)  {
            <span><img width=100 height=100>@album.Name<a href="/view_album?id=@album.AlbumId">View</a><a href="/edit_album?id=@album.AlbumId">Edit</a><a href="/delete_album?id=@album.AlbumId">Delete</a></span>
        }
    </body>    
</html>